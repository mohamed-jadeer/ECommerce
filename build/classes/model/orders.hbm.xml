<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping>
<class name="model.Orders" table="orders">
<composite-id>
<key-property name="orderId" column="order_id" type="integer" />
<key-property name="productId" column="product_id" type="integer" />
</composite-id>
<property name="productName" column="product_name" type="string" />
<property name="unitPrice" column="unit_price" type="float" />
<property name="orderQuantity" column="order_quantity" type="integer" />
<property name="netAmount" column="net_amount" type="float" />
</class>
</hibernate-mapping>